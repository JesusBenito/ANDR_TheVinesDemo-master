                                <ImageView
                                    android:id="@+id/btn_viviomuerto"
                                    android:layout_width="match_parent"
                                    android:layout_height="240dp"
                                    android:background="@drawable/veintiuno"
                                    android:src="@drawable/logovivoomuerto" />

                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

